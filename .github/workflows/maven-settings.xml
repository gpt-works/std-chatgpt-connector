<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">

  <profiles>
    <profile>
      <id>github</id>
      <repositories>
        <repository>
          <id>central</id>
          <url>https://repo1.maven.org/maven2</url>
          <releases><enabled>true</enabled></releases>
          <snapshots><enabled>true</enabled></snapshots>
        </repository>
        <repository>
          <id>github-std-chatgpt-connector</id>
          <name>GPT connector Commons Repository</name>
          <url>https://maven.pkg.github.com/gpt-works/github-std-chatgpt-connector</url>
          <releases><enabled>true</enabled></releases>
          <snapshots><enabled>true</enabled></snapshots>
        </repository>
        <repository>
          <id>github-gpt-connector-commons</id>
          <name>GPT connector Commons Repository</name>
          <url>https://maven.pkg.github.com/gpt-works/gpt-connector-commons</url>
          <releases><enabled>true</enabled></releases>
          <snapshots><enabled>true</enabled></snapshots>
        </repository>
        <repository>
          <id>github-pray</id>
          <name>GPT connector Commons Repository</name>
          <url>https://maven.pkg.github.com/gpt-works/pray</url>
          <releases><enabled>true</enabled></releases>
          <snapshots><enabled>true</enabled></snapshots>
        </repository>
      </repositories>
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>github</activeProfile>
  </activeProfiles>

  <servers>
    <server>
      <id>github-gpt-connector-commons</id>
      <username>${env.USER_NAME}</username>
      <password>${env.ACCESS_TOKEN}</password>
    </server>
    <server>
      <id>github-pray</id>
      <username>${env.USER_NAME}</username>
      <password>${env.ACCESS_TOKEN}</password>
    </server>
    <server>
      <id>github-std-chatgpt-connector</id>
      <username>${env.USER_NAME}</username>
      <password>${env.ACCESS_TOKEN}</password>
    </server>
  </servers>
</settings>
